
abstract class TwoDimensionalShapes extends Shapes {

		
private int dimension1, dimension2;
	
	public void TwoDimensionalShape(int x , int y, int d1, int d2)
	{
		super(x,y);
		dimension1 = d1;
		dimension2 = d2;
	}
	public void setDimension1(int d)
	{
		dimension1 = d;
	}

	public void setDimension2(int d)
	{
		dimension2 = d;
	}
	public int getDimension1()
	{
		return dimension1;
	}
	public int getDimension2()
	{
		return dimension2;
	}
	public abstract int area();
	
}
